<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wechat.gatingsystem.dao.OpenRecordDAO">
	<!-- mybatis 映射文件 相当于DAO的实现 每一个id相当于一个方法 -->

	<select id="selectByRecordID" parameterType="int" resultType="openRecord">
		select * from  open_record where record_id = #{recordID}
	</select>

</mapper>